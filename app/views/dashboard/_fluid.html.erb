<div class="tabbable black-box" style="margin-bottom: 18px;">

  <div class="tab-header">
    Fluid System
  </div>

  <div class="tab-content">
    <div class="tab-pane active" id="tab1">
      <div class="row-fluid">
        <div class="span6">
          <div class="padded">
            <div class="inner-well">
              <b>Temperature (&deg;C)</b>
              <span class="fluid-temp-value pull-right"></span>
            </div>
            <div class="inner-well">
              <b>Dissolved Solids (ppm)</b>
              <span class="fluid-tds-value pull-right"></span>
            </div>
            <div class="inner-well">
              <b>pH</b>
              <span class="fluid-ph-value pull-right"></span>
            </div>
            <div class="inner-well">
              <b>Line Pressure</b>
              <span class="fluid-line-pressure-value pull-right"></span>
            </div>
          </div>
        </div>
        <div class="span6">
          <div class="padded">
            <div class="inner-well">
              <b>Solenoid Valve</b>
              <span class="pull-right">
                <input type="checkbox" id="toggle-solenoid" class="checky" checked=true />
                <label for="toggle-solenoid" class="checky"><span></span></label>
              </span>
            </div>
            <div class="inner-well">
              <b>Pump</b>
              <span class="pull-right">
                <input type="checkbox" id="toggle-pump" class="checky" />
                <label for="toggle-pump" class="checky"><span></span></label>
              </span>
            </div>
          </div>
        </div>
      </div>

      <div class="row-fluid">
        <div class="span6">
          <div class="padded">
            <div class="inner-well">
              <div id="fluid-chart-line-pressure" class="chart" style="width: 100%; height: 200px;"></div>
            </div>
          </div>
          <div class="padded">
            <div class="inner-well">
              <div id="fluid-chart-temp" class="chart" style="width: 100%; height: 200px;"></div>
            </div>
          </div>
        </div>
        <div class="span6">
          <div class="padded">
            <div class="inner-well">
              <div id="fluid-chart-ph" class="chart" style="width: 100%; height: 200px;"></div>
            </div>
          </div>
          <div class="padded">
            <div class="inner-well">
              <div id="fluid-chart-tds" class="chart" style="width: 100%; height: 200px;"></div>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>
</div>
